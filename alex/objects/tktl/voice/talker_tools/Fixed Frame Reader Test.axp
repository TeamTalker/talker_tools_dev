   <patchobj type="patch/object" uuid="cd5971e6-cf05-4eab-83c9-af3380f34c5d" name="hex_10" x="448" y="364">
      <params/>
      <attribs/>
      <object id="patch/object" uuid="cd5971e6-cf05-4eab-83c9-af3380f34c5d">
         <sDescription>Pointless VU display of Talker LPC indices.
Part of the Talkie Tools object set.</sDescription>
         <author>Team Talker</author>
         <license>BSD</license>
         <helpPatch>disp.axh</helpPatch>
         <inlets>
            <frac32buffer.bipolar name="TALKER"/>
         </inlets>
         <outlets/>
         <displays>
            <frac32.vu name="energy"/>
            <frac32.vu name="pitch"/>
            <frac32.vu name="k0"/>
            <frac32.vu name="k1"/>
            <frac32.vu name="k2"/>
            <frac32.vu name="k3"/>
            <frac32.vu name="k4"/>
            <frac32.vu name="k5"/>
            <frac32.vu name="k6"/>
            <frac32.vu name="k7"/>
            <frac32.vu name="k8"/>
            <frac32.vu name="k9"/>
         </displays>
         <params/>
         <attribs/>
         <includes/>
         <code.krate><![CDATA[// START K-RATE

disp_energy = inlet_TALKER[0]  << 22;
disp_pitch  = inlet_TALKER[1]  << 20;
disp_k0     = inlet_TALKER[2]  << 21;
disp_k1     = inlet_TALKER[3]  << 21;
disp_k2     = inlet_TALKER[4]  << 22;
disp_k3     = inlet_TALKER[5]  << 22;
disp_k4     = inlet_TALKER[6]  << 22;
disp_k5     = inlet_TALKER[7]  << 22;
disp_k6     = inlet_TALKER[8]  << 22;
disp_k7     = inlet_TALKER[9]  << 23;
disp_k8     = inlet_TALKER[10] << 23;
disp_k9     = inlet_TALKER[11] << 23;

// END K-RATE]]></code.krate>
      </object>
   </patchobj>
